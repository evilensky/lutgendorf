<div class="row main-nav">
  <div class="col-md-12">
    <img src="/images/logo.png" style="width:60px; height:58px; float:left;margin-top:10px; margin-bottom:10px;"><h1>LivingWell</h1>
  </div>
  <div class="col-md-12">
    <div class="well">
      <h2>Welcome, <%=current_participant.email%>!</h2>
    </div>
    <div class="row">
      <div class="col-md-12">
        <% AppSection.all.each do |s| %>
          <% if s.label != "Qualtrics" %>
            <%= link_to("<div class='main-nav-button'>#{ s.label }</div>".html_safe, navigator_context_path(context_name: s.slug)) %>
            <%end%>
            
            <% if s.label == "Qualtrics" && s.qualtrics_visible?(current_participant) %>
              <%= link_to("#{ s.label }".html_safe, "http://google.com", :class=> 'qualtrics-link btn btn-danger') %>
            </div>
            <% end%>
        <% end %>
    </div>
  </div>
</div>

